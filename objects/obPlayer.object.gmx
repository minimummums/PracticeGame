            <string>///Player direction reset and position lock
obPlayer.x = clamp(obPlayer.x,0,room_width);

leftck= keyboard_check(!vk_left);
rightck= keyboard_check(!vk_right);

if(leftck &amp;&amp; rightck){
   if(obPlayer.direction &gt; 0 &amp;&amp; obPlayer.direction &lt; 180){
      obPlayer.direction = obPlayer.direction+(-obPlayer.direction)/4;
      obPlayer.image_angle = obPlayer.direction+(-obPlayer.image_angle)/4;
    }else if(obPlayer.direction &gt; 180 &amp;&amp; obPlayer.direction &lt; 360){
      obPlayer.direction = obPlayer.direction +(360-obPlayer.direction)/4;
      obPlayer.image_angle = obPlayer.direction +(360-obPlayer.image_angle)/4;
    }
    spd =0;
}

if(damageFlash){
    obPlayer.image_alpha = 0.5*sin(flashCount*1) + 0.5;
    flashCount++;
}
if(flashCount == 100){
    damageFlash = false;
    obPlayer.image_alpha = 1;
    flashCount = 0;
}

</string>
